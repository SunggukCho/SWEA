   "source": [
    "T = int(input())\n",
    "for i in range(T):\n",
    "    numbers = list(map(int, input().split()))\n",
    "    \n",
    "    avg_numbers = sum(numbers) / len(numbers)\n",
    "   \n",
    "    print('#{} {}'.format(i+1, avg_numbers))\n",
    "    i += 1"
   ]
